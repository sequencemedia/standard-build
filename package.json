{
  "name": "@sequencemedia/standard-build",
  "version": "1.1.35",
  "description": "Standard Build",
  "private": true,
  "main": "./main.mjs",
  "type": "module",
  "author": {
    "name": "Jonathan Perry for Sequence Media Limited",
    "email": "sequencemedia@sequencemedia.net",
    "url": "http://sequencemedia.net"
  },
  "license": "UNLICENSED",
  "scripts": {
    "build": "cross-env NODE_ENV=production concurrently -r \"npm run gulp -- build\" \"webpack --env prod --config webpack.prod.mjs\"",
    "build:css": "npm run nvm && cross-env NODE_ENV=development npm run gulp -- build:css",
    "build:font": "npm run nvm && cross-env NODE_ENV=development npm run gulp -- build:font",
    "build:img": "npm run nvm && cross-env NODE_ENV=development npm run gulp -- build:img",
    "build:js": "npm run nvm && cross-env NODE_ENV=development webpack --config webpack.dev.mjs",
    "dev": "cross-env NODE_ENV=development concurrently -r \"npm run gulp\" \"webpack --config webpack.dev.mjs --watch\"",
    "gulp": "gulp --color ",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:css": "stylelint source/assets/sass",
    "lint:css:fix": "npm run lint:css -- --fix",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint . --ext .mjs --ext .cjs",
    "lint:js:fix": "npm run lint:js -- --fix",
    "nvm": "bash nvm.sh",
    "prepare": "husky install",
    "start": "cross-env NODE_ENV=production DEBUG=@sequencemedia node app",
    "test": "cross-env NODE_ENV=test mocha test --recursive --verbose --color",
    "watch": "npm run nvm && cross-env NODE_ENV=development concurrently -r \"npm run gulp -- watch\" \"webpack --config webpack.dev.mjs --watch\"",
    "watch:css": "npm run nvm && cross-env NODE_ENV=development npm run gulp -- watch:css",
    "watch:font": "npm run nvm && cross-env NODE_ENV=development npm run gulp -- watch:font",
    "watch:img": "npm run nvm && cross-env NODE_ENV=development npm run gulp -- watch:img",
    "watch:js": "npm run nvm && cross-env NODE_ENV=development webpack --config webpack.dev.mjs --watch"
  },
  "dependencies": {
    "@sequencemedia/gulp": "^1.1.290",
    "@sequencemedia/gulp-clean-css": "^1.0.196",
    "@sequencemedia/gulp-css-purge": "^1.1.219",
    "@sequencemedia/gulp-debug": "^1.0.207",
    "@sequencemedia/gulp-postcss": "^1.0.219",
    "@sequencemedia/gulp-rename": "^1.0.205",
    "@sequencemedia/gulp-sass": "^1.0.201",
    "@sequencemedia/gulp-sourcemaps": "^1.0.209",
    "clean-webpack-plugin": "^4.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "debug": "^4.4.0",
    "del": "6.1.1",
    "dotenv": "^16.4.7",
    "glob-all": "^3.3.1",
    "module-alias": "^2.2.3",
    "postcss": "^8.4.49",
    "postcss-scss": "^4.0.9",
    "sass": "^1.83.1",
    "stylelint": "^16.12.0",
    "stylelint-config-recommended-scss": "^14.1.0",
    "stylelint-scss": "^6.10.0",
    "terser-webpack-plugin": "^5.3.11",
    "vinyl-paths": "3.0.1",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1",
    "yargs-parser": "^21.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.0",
    "@babel/eslint-parser": "^7.25.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-export-default-from": "^7.25.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.25.9",
    "@babel/preset-env": "^7.26.0",
    "@babel/register": "^7.25.9",
    "@sequencemedia/hooks": "^1.0.593",
    "autoprefixer": "^10.4.20",
    "babel-loader": "^9.2.1",
    "babel-plugin-minify-dead-code-elimination": "^0.5.2",
    "babel-plugin-module-resolver": "^5.0.2",
    "chai": "^5.1.2",
    "concurrently": "^9.1.2",
    "core-js": "^3.40.0",
    "eslint": "8.57.1",
    "eslint-config-standard": "^17.1.0",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "husky": "^9.1.7",
    "mocha": "^11.0.1",
    "sinon": "^19.0.2",
    "sinon-chai": "^4.0.0"
  },
  "imports": {
    "#source/build": "./source/build/index.mjs",
    "#source/build/*": "./source/build/*.mjs",
    "#source/build/args": "./source/build/args/index.mjs",
    "#source/build/args/*": "./source/build/args/*.mjs",
    "#source/build/args/assets": "./source/build/args/assets/index.mjs",
    "#source/build/paths": "./source/build/paths/index.mjs",
    "#source/build/paths/assets": "./source/build/paths/assets/index.mjs"
  }
}
